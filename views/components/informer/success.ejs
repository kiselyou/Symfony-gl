<div class="informer informer_xs">
    <div class="informer__title" data-block="header">
        <span>Success:</span>
    </div>
    <ul>
        <% for (let message of options.messages) { %>
        <li>
            <i class="fa fa-exclamation-triangle informer__icon informer__icon_success"></i>
            <span><%= message %></span>
        </li>
        <% } %>
    </ul>
</div>
