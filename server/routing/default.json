{
  "index": {
    "route": "/",
    "viewPath": "/controller/main/index.html",
    "method": "GET"
  },
  "user": {
    "route": "/home",
    "viewPath": "/controller/home/user.html",
    "method": "GET"
  },
  "play": {
    "route": "/play",
    "viewPath": "/pages/play.html",
    "method": "GET"
  },
  "dev": {
    "route": "/dev",
    "viewPath": "/pages/dev.html",
    "method": "GET"
  },
  "templates": {
    "route": "/template",
    "method": "POST",
    "controller": "systems:TemplateController:render"
  },
  "socket": {
    "route": "/socket/info",
    "method": "POST",
    "controller": "systems:SocketController:configuration"
  },

  "activation": {
    "route": "/iw/activation",
    "method": "GET",
    "controller": "systems:SecurityController:activation"
  },

  "login": {
    "route": "/iw/login",
    "method": "POST",
    "controller": "systems:SecurityController:login"
  },

  "logout": {
    "route": "/iw/logout",
    "method": "POST",
    "controller": "systems:SecurityController:logout"
  },

  "authenticated": {
    "route": "/iw/authenticated",
    "method": "POST",
    "controller": "systems:SecurityController:authenticated"
  },

  "registration": {
    "route": "/iw/registration",
    "method": "POST",
    "controller": "systems:SecurityController:registration"
  }
}
